{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "cviebrock/eloquent-sluggable": "^4.8",
        "fideloper/proxy": "^4.0",
        "jrm2k6/cloudder": "^0.6.0",
        "laravel/framework": "5.8.*",
        "laravel/nova": "~2.0",
        "laravel/scout": "^7.1",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "nothingworks/blade-svg": "^0.3.1",
        "realrashid/sweet-alert": "^1.1",
        "spatie/laravel-backup": "^6.2",
        "spatie/laravel-feed": "^2.2",
        "spatie/laravel-honeypot": "^1.3",
        "stevebauman/purify": "^2.0",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "brainmaestro/composer-git-hooks": "^2.7",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "wnx/laravel-stats": "^1.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Utilities/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "echo committing as $(git config user.name)"
            ],
            "pre-push": [
                "phpunit",
                "echo running prod yarn flow",
                "yarn prod",
                "echo finished running prod yarn flow"
            ]
        }
    }
}
